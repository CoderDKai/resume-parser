{
  "name": "resume-parser",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "start:mail-server": "turbo run start --filter=@resume-parser/mail-server",
    "dev:mail-server": "turbo run dev --filter=@resume-parser/mail-server",
    "add-dep:mail-server": "pnpm --filter=@resume-parser/mail-server add ",
    "add-dev-dep:mail-server": "pnpm --filter=@resume-parser/mail-server add -D ",
    "remove-dep:mail-server": "pnpm --filter=@resume-parser/mail-server remove",
    "start:pdf-processor": "turbo run start --filter=@resume-parser/pdf-processor",
    "dev:pdf-processor": "turbo run dev --filter=@resume-parser/pdf-processor",
    "add-dep:pdf-processor": "pnpm --filter=@resume-parser/pdf-processor add ",
    "add-dev-dep:pdf-processor": "pnpm --filter=@resume-parser/pdf-processor add -D ",
    "remove-dep:pdf-processor": "pnpm --filter=@resume-parser/pdf-processor remove "
  },
  "devDependencies": {
    "dotenv": "^16.4.7",
    "tsx": "^4.19.3",
    "turbo": "latest",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@10.2.1"
}
